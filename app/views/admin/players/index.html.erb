<div class="main-container">
  <h1>Admin Dashboard</h1>

  <div class="homepage-main-content">
    <ol>
      <% @players.order('last_name asc').each do |player| %>
        <li>
          <%= link_to admin_team_player_path(player.team_id, player.id) do %>
            <%= player.print_full_name %>
          <% end %>
        </li>
      <% end %>
    </ol>
  </div>
</div>